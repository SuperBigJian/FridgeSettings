public class WeightData {
    private int weight1;//称重专区1重量
    private boolean isPositive1;//称重专区1是否是正数
    private int weight2;//称重专区2重量
    private boolean isPositive2;//称重专区2是否是正数
    private int weight3;//称重专区3重量
    private boolean isPositive3;//称重专区3是否是正数

    public int getWeight1() {
        return weight1;
    }

    public void setWeight1(int weight1) {
        this.weight1 = weight1;
    }

    public boolean isPositive1() {
        return isPositive1;
    }

    public void setPositive1(boolean positive1) {
        isPositive1 = positive1;
    }

    public int getWeight2() {
        return weight2;
    }

    public void setWeight2(int weight2) {
        this.weight2 = weight2;
    }

    public boolean isPositive2() {
        return isPositive2;
    }

    public void setPositive2(boolean positive2) {
        isPositive2 = positive2;
    }

    public int getWeight3() {
        return weight3;
    }

    public void setWeight3(int weight3) {
        this.weight3 = weight3;
    }

    public boolean isPositive3() {
        return isPositive3;
    }

    public void setPositive3(boolean positive3) {
        isPositive3 = positive3;
    }
}
