// IPadControl.aidl
package com.midea.fridge.servant.fridgecontrol;

// Declare any non-default types here with import statements

interface IPadControl {
    /** 开关手势识别功能，返回值非0则操作失败 */
    int setGesture(int value);
    /** 获取手势开关状态 */
    int getGesture();
    /** 开侧灯 */
    int setSideLight(int id);
    int setBackLightOn();
    int setBackLightOff();
    int setSilentReboot(int value);
    int enableIR();
    int disableIR();
    int setIR(int value);
    int getIR();
}
