package com.midea.fridge.utils.fridgecontrolutil;

/**
 * 冰箱附加功能状态
 * Created by Administrator on 2016/9/6.
 */
public class FridgeExtendStatus {
    private boolean weighArea1Func;//称重专区1是否正常
    private boolean weighArea2Func;//称重专区2是否正常
    private boolean weighArea3Func;//称重专区3是否正常
    private boolean forceDefrostingFreeze;//强制冷冻室化霜
    private boolean forceDefrostingCool;//强制冷藏室化霜
    private boolean forceDefrostingVary;//强制变温室化霜
    private boolean forceDefrostingAll;//强制全间室化霜
    private boolean forceRun;//强制运行
    private byte[] data;

    public boolean isWeighArea1Func() {
        return weighArea1Func;
    }

    public void setWeighArea1Func(boolean weighArea1Func) {
        this.weighArea1Func = weighArea1Func;
    }

    public boolean isWeighArea2Func() {
        return weighArea2Func;
    }

    public void setWeighArea2Func(boolean weighArea2Func) {
        this.weighArea2Func = weighArea2Func;
    }

    public boolean isWeighArea3Func() {
        return weighArea3Func;
    }

    public void setWeighArea3Func(boolean weighArea3Func) {
        this.weighArea3Func = weighArea3Func;
    }

    public boolean isForceDefrostingFreeze() {
        return forceDefrostingFreeze;
    }

    public void setForceDefrostingFreeze(boolean forceDefrostingFreeze) {
        this.forceDefrostingFreeze = forceDefrostingFreeze;
    }

    public boolean isForceDefrostingCool() {
        return forceDefrostingCool;
    }

    public void setForceDefrostingCool(boolean forceDefrostingCool) {
        this.forceDefrostingCool = forceDefrostingCool;
    }

    public boolean isForceDefrostingVary() {
        return forceDefrostingVary;
    }

    public void setForceDefrostingVary(boolean forceDefrostingVary) {
        this.forceDefrostingVary = forceDefrostingVary;
    }

    public boolean isForceDefrostingAll() {
        return forceDefrostingAll;
    }

    public void setForceDefrostingAll(boolean forceDefrostingAll) {
        this.forceDefrostingAll = forceDefrostingAll;
    }

    public boolean isForceRun() {
        return forceRun;
    }

    public void setForceRun(boolean forceRun) {
        this.forceRun = forceRun;
    }

    public byte[] getData() {
        return data;
    }

    public void setData(byte[] data) {
        this.data = data;
    }
}
