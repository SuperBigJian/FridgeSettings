<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:switchStyle">@style/CustomSwitchStyle</item>
    </style>

    <style name="WifiDialogInfoTableTextView">
        <item name="android:textColor">#9B9B9B</item>
        <item name="android:textSize">@dimen/content_text</item>
    </style>

    <style name="WifiDialogInfoTable">
        <item name="android:layout_marginTop">20px</item>
    </style>

    <style name="CustomSwitchStyle" parent="Widget.AppCompat.CompoundButton.Switch">
        <item name="android:thumb">@drawable/switch_thumb</item>
        <item name="android:track">@drawable/switch_track</item>
        <item name="android:textOn">""</item>
        <item name="android:textOff">""</item>
    </style>

    <style name="DeviceInfoTableTitle">
        <item name="android:textColor">#676B6C</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">100px</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">@dimen/content_text</item>
        <item name="android:layout_marginStart">20px</item>
    </style>

    <style name="DeviceInfoTableContent">
        <item name="android:textColor">#9B9B9B</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">100px</item>
        <item name="android:gravity">center_vertical|end</item>
        <item name="android:layout_marginEnd">20px</item>
        <item name="android:textSize">@dimen/content_text</item>
    </style>

    <style name="SystemSettingsTextStyle">
        <item name="android:textColor">#4189a2</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">100px</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textSize">30px</item>
    </style>

    <style name="QQ">
        <item name="android:textColor">#0F1F2E</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20px</item>
    </style>

    <style name="Area">
        <item name="android:textColor">#676B6C</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">20px</item>
        <item name="android:paddingStart">10px</item>
        <item name="android:textSize">15px</item>
    </style>


    <style name="Dialog" parent="android:style/Theme.Dialog">
        <item name="android:background">#00000000</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>

</resources>
